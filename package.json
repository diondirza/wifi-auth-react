{
  "name": "wifi-auth",
  "version": "1.0.0",
  "description": "Midtrans Wi-fi Authentication page",
  "scripts": {
    "start": "./node_modules/.bin/babel-node ./server.js",
    "clean": "rimraf ./public",
    "server": "npm run server:dev",
    "server:dev": "webpack-dev-server --config config/webpack.dev.js --inline --progress --profile --colors --watch --display-error-details --display-cached --content-base src/",
    "server:dev:hmr": "npm run server:dev -- --hot",
    "server:prod": "NODE_ENV=production npm start",
    "dev": "npm run build:dev",
    "prod": "npm run build:prod",
    "watch": "npm run watch:dev",
    "watch:dev": "npm run build:dev -- --watch",
    "watch:dev:hmr": "npm run watch:dev -- --hot",
    "watch:prod": "npm run build:prod -- --watch",
    "build": "npm run build:dev",
    "prebuild:dev": "npm run clean",
    "build:dev": "webpack --config ./config/webpack.dev.js --progress --profile --colors --display-error-details --display-cached",
    "prebuild:prod": "npm run clean",
    "build:prod": "webpack --config ./config/webpack.prod.js --progress --profile --colors --display-error-details --display-cached --bail"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/diondirza/wifi-auth-react.git"
  },
  "keywords": [
    "Midtrans",
    "WiFi",
    "Authentication"
  ],
  "author": "Dion Dirza",
  "license": "ISC",
  "engines": {
    "node": ">= 6",
    "npm": ">= 3"
  },
  "bugs": {
    "url": "https://github.com/diondirza/wifi-auth-react/issues"
  },
  "homepage": "https://github.com/diondirza/wifi-auth-react#readme",
  "dependencies": {
    "babel-cli": "^6.10.1",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-require": "^1.0.1",
    "classnames": "^2.2.5",
    "compression": "^1.6.2",
    "events": "^1.1.1",
    "express": "^4.14.0",
    "express-history-api-fallback": "^2.0.0",
    "jquery": "^3.1.0",
    "materialize-css": "^0.97.6",
    "react": "^15.2.1",
    "react-dom": "^15.2.1",
    "react-router": "^2.5.2",
    "typed.js": "^1.1.1"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.7",
    "babel-eslint": "^6.1.2",
    "compression-webpack-plugin": "^0.3.1",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.23.1",
    "eslint": "^3.0.1",
    "eslint-loader": "^1.4.1",
    "eslint-plugin-react": "^5.2.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.22.0",
    "image-webpack-loader": "^2.0.0",
    "imports-loader": "^0.6.5",
    "node-sass": "^3.8.0",
    "postcss-loader": "^0.9.1",
    "react-hot-loader": "^1.3.0",
    "resolve-url-loader": "^1.6.0",
    "rimraf": "^2.5.3",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-cleanup-plugin": "^0.2.0",
    "webpack-dev-server": "^1.14.1",
    "webpack-md5-hash": "0.0.5",
    "webpack-merge": "^0.14.0"
  }
}
